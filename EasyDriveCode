CREATE TABLE Clients (
    clientNo INT PRIMARY KEY,
    fName VARCHAR(255),
    lName VARCHAR(255),
    dob DATE,
    transportingInstructorNo INT,
    requestedInstructorNo INT,
    addressId INT,
    gender CHAR(1),
    FOREIGN KEY (transportingInstructorNo) REFERENCES Staff(staffId)
);

