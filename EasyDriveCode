CREATE TABLE InterviewDetails (
    clientNo INT,
    staffId INT,
    interviewDate DATE,
    notes VARCHAR(255),
    PRIMARY KEY (clientNo, staffId, interviewDate),
    FOREIGN KEY (clientNo) REFERENCES Clients(clientNo),
    FOREIGN KEY (staffId) REFERENCES Staff(staffId)
);


