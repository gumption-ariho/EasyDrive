	CREATE TABLE Telephone(
telephoneNo varChar(12) PRIMARY KEY,
staffId INT,
FOREIGN KEY (staffId) REFERENCES Staff(staffId),
CONSTRAINT CHK_TelephoneNo_Format CHECK (telephoneNo LIKE '[0-9][0-9][0-9]-[0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]')
)

