CREATE TABLE Address (
    addressId INT PRIMARY KEY,
    StreetNo VARCHAR(20),
    StreetName VARCHAR(100),
    cityId INT,
    zip VARCHAR(20),
    FOREIGN KEY (cityId) REFERENCES City(cityId)
);

