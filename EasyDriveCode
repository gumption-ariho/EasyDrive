CREATE TABLE ClientRegistration (
    formNo INT PRIMARY KEY,
    registrationDate DATE,
    officeId INT,
    clientNo INT,
    FOREIGN KEY (officeId) REFERENCES Offices(officeId),
    FOREIGN KEY (clientNo) REFERENCES Clients(clientNo)
);

