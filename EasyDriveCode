CREATE TABLE Test (
    testNo INT PRIMARY KEY,
    testDate DATE,
    clientNo INT,
    status VARCHAR(50),
    CommentOnResult TEXT,
    FOREIGN KEY (clientNo) REFERENCES Clients(clientNo)
);

